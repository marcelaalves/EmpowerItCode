<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/mainLinear"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@android:color/white"
	android:orientation="vertical">

	<include layout="@layout/title_bar"></include>


	<LinearLayout
		android:id="@+id/iid"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_marginBottom="20dp"
		android:layout_weight="47.39"
		android:gravity="center"
		android:orientation="horizontal"
		android:paddingTop="10dp"
		android:weightSum="100">

		<RelativeLayout
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="50"
			android:gravity="center"
			android:orientation="horizontal">

			<ImageView
				android:id="@+id/image_wob"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/corp">

			</ImageView>
		</RelativeLayout>

		<RelativeLayout
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="50"
			android:gravity="center"
			android:orientation="vertical">


			<LinearLayout
				android:id="@+id/w10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_margin="10dp"

				android:orientation="vertical">

				<TextView
					android:id="@+id/widget37"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_alignParentTop="true"
					android:layout_centerHorizontal="true"
					android:layout_x="153dp"
					android:layout_y="49dp"
					android:text="The WOB"
					android:textColor="#152D53"
					android:textSize="20sp"
					android:textStyle="bold" />

				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="5dp"
					android:layout_alignParentTop="true"
					android:layout_centerHorizontal="true"
					android:background="#8191b2"
					android:orientation="horizontal"></LinearLayout>
			</LinearLayout>

			<LinearLayout
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:layout_below="@+id/w10"
				android:orientation="horizontal"
				android:weightSum="1">

				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="0.50">

					<ImageButton
						android:id="@+id/imageButton"
						android:layout_width="68dp"
						android:layout_height="49dp"
						android:layout_gravity="center_vertical"
						android:background="@drawable/ic_activities" />
				</LinearLayout>


				<LinearLayout
					android:layout_width="5dp"
					android:layout_height="match_parent"
					android:background="#8191b2"
					android:orientation="vertical"></LinearLayout>

				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_weight="0.50">

					<ImageButton
						android:layout_marginLeft="10dp"
						android:id="@+id/imageButton2"
						android:layout_width="64dp"
						android:layout_height="50dp"
						android:layout_gravity="center_vertical"
						android:background="@drawable/ic_news" />
				</LinearLayout>

			</LinearLayout>

		</RelativeLayout>

	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="5dp"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:background="#8191b2"
		android:orientation="horizontal" />

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_marginBottom="20dp"
		android:layout_weight="50.00"
		android:gravity="center"
		android:orientation="horizontal"
		android:weightSum="100">


		<RelativeLayout
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="50"
			android:gravity="center"
			android:orientation="vertical">

			<Button
				android:id="@+id/btnSearch"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@drawable/ic_search"
				android:padding="5dp"
				android:layout_centerVertical="true"
				android:layout_alignRight="@+id/textView"
				android:layout_alignEnd="@+id/textView" />

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:text="Search partners"
				android:id="@+id/textView"
				android:textColor="#c91d64"
				android:layout_alignParentBottom="true"
				android:layout_centerHorizontal="true" />

		</RelativeLayout>

		<LinearLayout
			android:layout_width="5dp"
			android:layout_height="match_parent"
			android:layout_marginBottom="5dp"
			android:layout_marginTop="5dp"
			android:background="#8191b2"
			android:orientation="vertical" />

		<RelativeLayout
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="50"
			android:gravity="center"
			android:orientation="vertical">

			<Button
				android:id="@+id/btnMeusCursos2"
				android:layout_width="93dp"
				android:layout_height="93dp"
				android:background="@drawable/ic_mybusiness"
				android:padding="5dp"
				android:layout_centerVertical="true"
				android:layout_alignParentTop="true"
				android:layout_centerHorizontal="true" />


			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:text="My business"
				android:id="@+id/textView2"
				android:textColor="#c91d64"
				android:layout_alignParentBottom="true"
				android:layout_centerHorizontal="true" />
		</RelativeLayout>

	</LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="5dp"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:background="#8191b2"
		android:orientation="horizontal"></LinearLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="50.00"
		android:baselineAligned="false"
		android:gravity="center"
		android:orientation="horizontal"
		android:paddingTop="10dp"
		android:weightSum="100"
		android:padding="15dp">

		<RelativeLayout
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="50"
			android:gravity="center"
			android:orientation="vertical">

			<Button
				android:id="@+id/btnMeusCurs22os"
				android:layout_width="93dp"
				android:layout_height="93dp"
				android:background="@drawable/ic_agenda"
				android:padding="5dp"
				android:layout_marginBottom="3dp"
				android:layout_centerVertical="true"
				android:layout_alignParentTop="true"
				android:layout_centerHorizontal="true" />

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:text="Agenda"
				android:id="@+id/textView3"
				android:textColor="#c91d64"
				android:layout_alignParentBottom="true"
				android:layout_centerHorizontal="true" />
		</RelativeLayout>

		<LinearLayout
			android:layout_width="5dp"
			android:layout_height="match_parent"
			android:layout_marginBottom="5dp"
			android:layout_marginTop="5dp"
			android:background="#8191b2"
			android:orientation="vertical" />

		<RelativeLayout
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="50"
			android:gravity="center"
			android:orientation="horizontal">

			<Button
				android:id="@+id/btnMeusCllursos"
				android:layout_width="93dp"
				android:layout_height="93dp"
				android:layout_centerInParent="true"
				android:background="@drawable/ic_settings"
				android:padding="5dp"
				android:layout_marginBottom="3dp"/>

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:text="Settings"
				android:id="@+id/textView4"
				android:textColor="#c91d64"
				android:layout_alignParentBottom="true"
				android:layout_centerHorizontal="true" />
		</RelativeLayout>

	</LinearLayout>

</LinearLayout>